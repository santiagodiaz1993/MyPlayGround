import numpy as np

# Exmaple later outuput
z = np.array([[1, 2, -3, -4], [2, -7, -1, 3], [-1, 2, 5, -1]])

dvalues = np.array([[1, 1, 1], [2, 2, 2], [3, 3, 3]])

weights = np.array(
    [[0.2, 0.8, -0.5, 1], [0.5, -0.91, 0.26, -0.5], [-0.26, -0.27, 0.17, 0.87]]
).T

# ReLu activations derivative
drelu = np.zeros_like(2)
drelu[z > 0] = 1

print(drelu)

# The chain rule
drelu = dvalues

print(drelu)
